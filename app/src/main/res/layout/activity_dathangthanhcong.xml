<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="736dp"
        android:background="@drawable/drawable_shape"
        tools:ignore="ExtraText,MissingConstraints">

        <TextView
            android:layout_width="203dp"
            android:layout_height="37dp"
            android:layout_marginLeft="98dp"
            android:layout_marginTop="216dp"
            android:layout_gravity="top|left"
            android:gravity="center_horizontal|center_vertical"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:singleLine="true"
            android:text="Food App"
            android:textColor="#FF000000"
            android:textSize="26dp" />

        <ImageView
            android:layout_width="96dp"
            android:layout_height="134dp"
            android:layout_marginLeft="152dp"
            android:layout_marginTop="57dp"
            android:layout_gravity="top|left"
            android:scaleType="centerInside"
            android:src="@drawable/logo" />

        <TextView
            android:layout_width="300dp"
            android:layout_height="138dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="299dp"
            android:layout_gravity="top|left"
            android:gravity="center_horizontal|top"
            android:includeFontPadding="false"
            android:lineHeight="24.2dp"
            android:text="Đơn hàng của bạn đã được đặt."
            android:textColor="#FF000000"
            android:textSize="20dp" />

        <FrameLayout
            android:layout_width="149dp"
            android:layout_height="37dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="516dp"
            android:layout_gravity="top|left"
            android:background="@drawable/image_rectangle1">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/container_group"
                android:scaleType="centerCrop" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="14dp"
                android:layout_marginTop="5.5dp"
                android:layout_marginBottom="-0.5dp"
                android:layout_gravity="top"
                android:gravity="center_horizontal|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Về trang chủ"
                android:textColor="#FF000000"
                android:textSize="20dp" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="149dp"
            android:layout_height="37dp"
            android:layout_marginLeft="226dp"
            android:layout_marginTop="516dp"
            android:layout_gravity="top|left"
            android:background="@drawable/image_rectangle">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/container_group1"
                android:scaleType="centerCrop" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:layout_marginLeft="34dp"
                android:layout_marginRight="32dp"
                android:layout_marginTop="5.5dp"
                android:layout_marginBottom="-0.5dp"
                android:layout_gravity="top"
                android:gravity="center_horizontal|center_vertical"
                android:includeFontPadding="false"
                android:maxLines="1"
                android:singleLine="true"
                android:text="Đánh giá"
                android:textColor="#FF000000"
                android:textSize="20dp" />
        </FrameLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>